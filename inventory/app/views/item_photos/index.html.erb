<p id="notice"><%= notice %></p>

<h1>Item Photos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @item_photos.each do |item_photo| %>
      <tr>
        <td><%= link_to 'Show', item_photo %></td>
        <td><%= link_to 'Edit', edit_item_photo_path(item_photo) %></td>
        <td><%= link_to 'Destroy', item_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item Photo', new_item_photo_path %>
