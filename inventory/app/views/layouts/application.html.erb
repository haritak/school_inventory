<!DOCTYPE html>
<html>
  <head>
    <title>Inventory</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <table>
      <tr>
        <td align=center>
          <small><%= link_to 'Users', users_path %></small>
        </td>
        <td align=center>
          <small><%= link_to 'Items', items_path %></small>
        </td>
      </tr>
    </table>
    <%= yield %>
  </body>
</html>
