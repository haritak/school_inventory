<p id="notice"><%= notice %></p>

<h1>Secondary Photos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @secondary_photos.each do |secondary_photo| %>
      <tr>
        <td><%= link_to 'Show', secondary_photo %></td>
        <td><%= link_to 'Edit', edit_secondary_photo_path(secondary_photo) %></td>
        <td><%= link_to 'Destroy', secondary_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Secondary Photo', new_secondary_photo_path %>
